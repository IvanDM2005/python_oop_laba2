class Employee:
name = ""
salary = ""
myEmployee1 = Employee()
myEmployee2 = Employee()
myEmployee3 = Employee()
myEmployee1.name = "Николай"
myEmployee2.name = "Степан"
myEmployee3.name = "Бобчик"
myEmployee1.salary = 150000
myEmployee2.salary = 123000
myEmployee3.salary = 500
print(myEmployee1.salary + myEmployee2.salary + myEmployee3.salary)
